  <style>
    .user-message {
      margin: auto;
      width: 40%;
      border: 1px solid #4771fa;
      padding: 0.5rem;
      border-radius: 3px;
      background: #b9c9ff;
      text-align: center;
    }
    
    .user-message--error {
      border-color: red;
      background: rgb(255, 176, 176);
      color: red;
      margin-bottom: 16px;
    }
  
    .invalid {
        display: block;
        width: 100%;
        height: calc(2.25rem + 2px);
        padding: 0.375rem 0.75rem;
        font-size: 0.9rem;
        line-height: 1.5;
        color: #495057;
        background-clip: padding-box;
        border: 1px solid #ced4da;
        border-radius: 0.25rem;
        transition: border-color .15s ease-in-out,box-shadow .15s ease-in-out;
        border-radius: 0;
        height: 40px;
        padding-left: 18px;
        border-color: transparent;
        background: rgba(130,139,178,0.1);
        border-color: red;
    }

    .logo {
    width: 40px; 
    height: 40px; 
    border-radius: 20px;
   }

   .logo1 {
    width: 50px; 
    height: 50px; 
    border-radius: 20px;
   }

   @media (max-width: 1085px) {
    .logo {
       display: none;
     }
   }
  </style>
