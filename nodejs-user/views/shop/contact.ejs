	<!-- ================ contact section start ================= -->
  <section class="section-margin--small">
    <div class="container">
      <div class="d-none d-sm-block mb-5 pb-4">
        <div id="map" style="height: 420px;"></div>
        <script>
          function initMap() {
            var uluru = {lat: -25.363, lng: 131.044};
            var grayStyles = [
              {
                featureType: "all",
                stylers: [
                  { saturation: -90 },
                  { lightness: 50 }
                ]
              },
              {elementType: 'labels.text.fill', stylers: [{color: '#A3A3A3'}]}
            ];
            var map = new google.maps.Map(document.getElementById('map'), {
              center: {lat: -31.197, lng: 150.744},
              zoom: 9,
              styles: grayStyles,
              scrollwheel:  false
            });
          }
          
        </script>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDpfS1oRGreGSBU5HHjMmQ3o5NLw7VdJ6I&callback=initMap"></script>
        
      </div>


      <div class="row">
        <div class="col-md-4 col-lg-3 mb-4 mb-md-0">
          <div class="media contact-info">
            <span class="contact-info__icon"><i class="ti-home"></i></span>
            <div class="media-body">
              <h3>Shopping Cart</h3>
              <p>Surat, Gujarat.</p>
            </div>
          </div>
          <div class="media contact-info">
            <span class="contact-info__icon"><i class="ti-headphone"></i></span>
            <div class="media-body">
              <h3><a href="#">125-668-886</a></h3>
              <p>Mon to Fri 9am to 6pm</p>
            </div>
          </div>
          <div class="media contact-info">
            <span class="contact-info__icon"><i class="ti-email"></i></span>
            <div class="media-body">
              <h3><a href="#">test@shopping.com</a></h3>
              <p>Send us your query anytime!</p>
            </div>
          </div>
        </div>
        <div class="col-md-8 col-lg-9">
          <% if (errorMessage) { %>
                <div class="user-message user-message--error"><%= errorMessage %></div>
          <% } %>
          <form action="/contact" class="form-contact contact_form" method="POST" novalidate>
            <div class="row">
              <div class="col-lg-5">
                <div class="form-group">
                  <input class="<%= validationErrors.find(e => e.param === 'fname') ? 'invalid' : 'form-control' %>" name="fname" id="fname" type="text" placeholder="Enter Your Name" value="<% if (hasError) { %><%= contact.fname %><% } %>">
                </div>
                <div class="form-group">
                  <input class="<%= validationErrors.find(e => e.param === 'email') ? 'invalid' : 'form-control' %>" name="email" id="email" type="email" placeholder="Enter Your Email" value="<% if (hasError) { %><%= contact.email %><% } %>">
                </div>
                <div class="form-group">
                  <input class="<%= validationErrors.find(e => e.param === 'phoneno') ? 'invalid' : 'form-control' %>" name="phoneno" id="phoneno" type="number" placeholder="Enter Your Phoneno" value="<% if (hasError) { %><%= contact.phoneno %><% } %>">
                </div>
              </div>
              <div class="col-lg-7">
                <div class="form-group">
                    <textarea class="<%= validationErrors.find(e => e.param === 'question') ? 'different-control w-100 invalid' : 'form-control different-control w-100' %>" name="question" id="question" cols="30" rows="5" placeholder="Enter Your Question"><% if (hasError) { %><%= contact.question %><% } %></textarea>
                </div>
              </div>
            </div>
            <div class="form-group text-center text-md-right mt-3">
              <input type="hidden" name="_csrf" value="<%= csrfToken %>">
              <button type="submit" class="button button--active button-contactForm">Send Question</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </section>
	<!-- ================ contact section end ================= -->
  
  

